{
  "version": "0.2.0",
  "configurations": [
    
    
    {
      "command": "npm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "command": "npm run debug-ide",
      "name": "run debug-ide",
      "request": "launch",
      "type": "node-terminal"
    },

    {
        "name": "Launch via NPM",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
            "run-script", "debug-ide"
        ],
        "port": 5858
    }
  ]
}